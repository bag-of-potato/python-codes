from os import abort, stat_result
import pyautogui as auto 
from time import sleep

status = True


while status:
    auto.moveTo(700,50,duration= 0.3)
    auto.click
    print(auto.position())
    sleep(3)
    auto.moveTo(700,700,duration= 0.3)
    auto.click
    print(auto.position())
    sleep(3)
    Status = False
    break

